{
    "name": "meklis/switcher-core-api",
    "description": "Switcher core API",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "meklis/switcher-core": "dev-master",
        "monolog/monolog": "*",
        "nyholm/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "spiral/goridge": "3.2.x-dev",
        "spiral/roadrunner": "v2.4.1",
        "spiral/roadrunner-metrics": "2.1.x-dev"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8082 -t public"
    },
    "minimum-stability": "dev"
}
